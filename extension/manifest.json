{
  "name": "IkTube",
  "description": "A simple YouTube downloading extension.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "webRequest",
    "storage"
  ]
}